<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DataCapture name="DC-TokenCounts" continueOnError="false" enabled="true">
  <DisplayName>DC-TokenCounts</DisplayName>
  <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <Capture>
    <Collect ref="genai.techmodel" default="0"/>
    <DataCollector>dc_genai_model_name</DataCollector>
  </Capture>
  <Capture>
    <Collect ref="genai.promptTokenCount" default="0"/>
    <DataCollector>dc_genai_prompt_tokens</DataCollector>
  </Capture>
  <Capture>
    <Collect ref="genai.responseTokenCount" default="0"/>
    <DataCollector>dc_genai_completion_tokens</DataCollector>
  </Capture>
  <Capture>
    <Collect ref="genai.totalTokenCount" default="0"/>
    <DataCollector>dc_genai_total_tokens</DataCollector>
  </Capture>
</DataCapture>
